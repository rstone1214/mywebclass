{
  "name": "MyWebClass.org",
  "version": "0.1.0",
  "description": "git collaboration using  ci/cd pipline and docker",
  "main": "src/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "npx standard && python -m pytest -c pytest.ini && npx playwright test --config=playwright.config.js",
    "install_backend": "python -m pip install -r requirements.txt",
    "start": "webpack serve --open --config webpack.config.js",
    "build": "webpack  --config webpack.config.js",
    "grade": "python -m pytest -c pytest_grade.ini && npx playwright test --config=playwright_grade.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NJIT-WIS/collab.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NJIT-WIS/collab/issues"
  },
  "homepage": "https://github.com/NJIT-WIS/collab#readme",
  "devDependencies": {
    "@playwright/test": "^1.30.0",
    "autoprefixer": "^10.4.13",
    "bootstrap": "^5.2.3",
    "css-loader": "^6.7.3",
    "favicons": "^7.0.2",
    "favicons-webpack-plugin": "^6.0.0",
    "html-webpack-plugin": "^5.5.0",
    "live-server": "^1.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "playwright": "^1.30.0",
    "postcss-loader": "^7.0.2",
    "sass": "^1.58.0",
    "sass-loader": "^13.2.0",
    "standard": "^17.0.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "bootstrap-icons": "^1.10.3"
  }
}
